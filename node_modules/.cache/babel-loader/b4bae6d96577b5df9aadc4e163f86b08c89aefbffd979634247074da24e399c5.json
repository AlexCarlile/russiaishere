cted,\n  ActionPage,\n  PrivateRoute,\n  AboutPage,\n  PersonalArea,\n  PartnersPage,\n} from \"./pages\";\nimport { Projects } from './pages';\nimport { AuthProvider, useAuth } from './store';\n\nexport const App: React.FC = () => {\n  const { authorized } = useAuth();\n  const navigate = useNavigate()\n\n  useEffect(() => {\n    console.log(\"Authorized:\", authorized);\n  }, [authorized]);\n  \n  return (\n    <Routes>\n        <Route path=\"/login\" element={<Login />} />\n        <Route path=\"/protected\" element={<Protected />} />\n        <Route\n          path='*'\n          element={\n            <PrivateRoute authorized={authorized}>\n              <InternalRoutes />\n            </PrivateRoute>\n          }\n        />\n    </Routes>\n  );\n}\n\nconst InternalRoutes = () => {\n  return(\n    <Routes>\n      <Route path=\"/\" element={<MainPage />}/>\n      <Route path=\"/actions\" element={<ActionPage />}/>\n      <Route path=\"/about\" element={<AboutPage />}/>\n      <Route path=\"/personal\" element={<PersonalArea />}/>\n      <Route path=\"/partners\" element={<PartnersPage />}/>\n      <Route path=\"/projects/:teamId\" element={<Projects />}/>\n\n    </Routes>\n\n  )\n}\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAGC,SAAS,KAAO,OAAO,CACtC,OAASC,MAAM,CAAEC,KAAK,CAAEC,WAAW,KAAQ,kBAAkB,CAC7D,OACEC,KAAK,CACLC,QAAQ,CACRC,SAAS,CACTC,UAAU,CACVC,YAAY,CACZC,SAAS,CACTC,YAAY,CACZC,YAAY,KACP,SAAS,CAChB,OAASC,QAAQ,KAAQ,SAAS,CAClC,OAAuBC,OAAO,KAAQ,SAAS,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAEhD,MAAO,MAAM,CAAAC,GAAa,CAAGA,CAAA,GAAM,CACjC,KAAM,CAAEC,UAAW,CAAC,CAAGN,OAAO,CAAC,CAAC,CAChC,KAAM,CAAAO,QAAQ,CAAGjB,WAAW,CAAC,CAAC,CAE9BH,SAAS,CAAC,IAAM,CACdqB,OAAO,CAACC,GAAG,CAAC,aAAa,CAAEH,UAAU,CAAC,CACxC,CAAC,CAAE,CAACA,UAAU,CAAC,CAAC,CAEhB,mBACEF,KAAA,CAAChB,MAAM,EAAAsB,QAAA,eACHR,IAAA,CAACb,KAAK,EAACsB,IAAI,CAAC,QAAQ,CAACC,OAAO,cAAEV,IAAA,CAACX,KAAK,GAAE,CAAE,CAAE,CAAC,cAC3CW,IAAA,CAACb,KAAK,EAACsB,IAAI,CAAC,YAAY,CAACC,OAAO,cAAEV,IAAA,CAACT,SAAS,GAAE,CAAE,CAAE,C